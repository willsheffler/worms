--geometry Cyclic(3)
--bbconn 
   _N HFuse_het_C2_N-N
   CC HFuse_het_C2_C-C
   N_ HFuse_het_C2_N-N

--monte_carlo 0
--no_duplicate_bases 0
--nbblocks 4
--shuffle_bblocks 0

--cachedirs ./testcache
--disable_cache 1

--parallel 0

--tolerance 1.0
--max_score0 10.0
--max_score0sym 50.0

--helixconf_min_num_horiz_helix 1
--helixconf_max_vert_angle 5
--helixconf_max_depth_within_hull 2
--helixconf_use_hull_for_surface 0
--helixconf_min_helix_size 14

#--only_bblocks 0 1 0



--pbar 0



--lever 25

--splice_rms_range 4
--splice_max_rms 0.8
--splice_clash_contact_by_helix=1
--splice_ncontact_cut 1
--splice_ncontact_no_helix_cut 1
--splice_nhelix_contacted_cut 1
--splice_max_chain_length 9999

--dbfiles 
   helixori_1result.txz

